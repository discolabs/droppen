<p id="notice"><%= notice %></p>

<h1>Listing Droppens</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @droppens.each do |droppen| %>
      <tr>
        <td><%= link_to 'Show', droppen %></td>
        <td><%= link_to 'Edit', edit_droppen_path(droppen) %></td>
        <td><%= link_to 'Destroy', droppen, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Droppen', new_droppen_path %>
